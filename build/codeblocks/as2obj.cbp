<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="as2obj" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/as2obj" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option additional_output="bin/Debug/liblc3.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="bin/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/as2obj" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option additional_output="bin/Release/liblc3.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O3" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="bin/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="$(#complx_deps.cflags)" />
			<Add directory="$(#complx_deps.include)" />
			<Add directory="../../liblc3" />
		</Compiler>
		<Linker>
			<Add option="-llc3" />
			<Add directory="$(#complx_deps.lib)" />
		</Linker>
		<Unit filename="../../as2obj/main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
